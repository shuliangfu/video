{
  "name": "@dreamer/video",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "exports": {
    ".": "./src/mod.ts",
    "./client": "./src/client/mod.ts"
  },
  "publish": {
    "include": [
      "src/**/*.ts",
      "src/**/*.json",
      "LICENSE",
      "NOTICE",
      "README.md"
    ],
    "exclude": [
      "**/*.test.ts",
      "**/*.spec.ts",
      "**/test/**",
      "**/tests/**"
    ]
  },
  "lint": {
    "include": [
      "src/"
    ],
    "rules": {
      "tags": [
        "recommended"
      ],
      "exclude": [
        "no-explicit-any"
      ]
    }
  },
  "imports": {
    "@dreamer/i18n": "jsr:@dreamer/i18n@^1.0.1",
    "@dreamer/runtime-adapter": "jsr:@dreamer/runtime-adapter@^1.0.15",
    "@dreamer/test": "jsr:@dreamer/test@^1.0.12"
  },
  "nodeModulesDir": "auto"
}
